<Files  ~ "\.ini$">
  Order allow,deny
  Deny from all
</Files>

<Files  ~ "\.twig$">
  Order allow,deny
  Deny from all
</Files>

<Files  ~ "\.sh$">
  Order allow,deny
  Deny from all
</Files>


RewriteEngine On
RewriteBase /


RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond $0#%{REQUEST_URI} ([^#]*)#(.*)\1$
RewriteRule ^.*$ %2index.php [QSA,L]

